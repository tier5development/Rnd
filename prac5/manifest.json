{
"manifest_version":2,

"name":"Experiment",
"description":"Playing around",
"version":"1.0",

"browser_action":{
    "default_icon":"logo.png",
    "default_popup":"popup.html"
},
"icons":{
    "64":"logo.png"
},
"background":{
    "scripts":["background.js"]
},
"content_scripts":[
    {
        "matches":["http://*/*","https://*/*"],
        "js":["myScript.js"]
    }
]
}