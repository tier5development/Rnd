{
    "manifest_version": 2,
    "name":"Toolbar",
    "version":"1.0",
    "description":"creating first toolbar",

    "browser_action":{
        "default_popup":"popup.html",
        "default_icon":"logo.png"
    },
    "icons":{
        "64":"logo.png"
    },
    "content_scripts":[
        {
            "matches":["http://*/*","https://*/*"],
            "css":["style.css"],
            "js":["jquery.js","myScript.js"]
        }
    ],
    "web_accessible_resources":[
        "toolbar.html",
        "style.css"
    ]

}