{
    "version":"1.0.0",
    "manifest_version":2,
    "name":"R&D Project",
    "description":"This is my R&D extension",
     "browser_action":{
         "default_icon":"icon.png",
         "default_popup": "popup.html"
     },
     "content_scripts": [
        {
          "matches": ["*://*.facebook.com/*", "*://facebook.com/*"],
          "js": ["jquery.min.js","content.js"]
        }
      ],
     "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
      "permissions": [
        "storage"
      ]
}