{
    "manifest_version": 3,
    "name": "Demo Extension",
    "version": "1.0.0",
    "description": "This is very cool extension",
    
    "permissions": [
        "bookmarks",
        "storage",
        "tabs"
    ],

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery.min.js",
                "helper.js",
                "renderDom.js",
                "contentScriptVersionA.js"
            ],
            "run_at": "document_end"
        }
    ],
    
    "action": {
        "default_title": "Developped By Ankur",
        "default_popup": "popup.html"
    }
}